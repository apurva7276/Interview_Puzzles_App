<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.interview_puzzle_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Interview_Puzzle_App"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".HintActivity"
            android:exported="false" />
        <activity
            android:name=".HelpDetails"
            android:exported="false"
            android:parentActivityName=".ui.logIn.SecondActivity" />
        <activity
            android:name=".ui.QuestionUI.QuestionUiActivity"
            android:exported="false" />
        <activity
            android:name=".ui.PracticeMode.QueList.PracticeQueListActivity"
            android:exported="false" />
        <activity
            android:name=".ui.Profile.changeAvtarActivity"
            android:exported="false"
            android:parentActivityName=".ui.Profile.Profile" />
        <activity
            android:name=".ui.Profile.editProfileActivity"
            android:exported="false"
            android:parentActivityName=".ui.Profile.Profile" />
        <activity
            android:name=".ui.Profile.Profile"
            android:exported="false"
            android:parentActivityName=".ui.logIn.SecondActivity" />
        <activity
            android:name=".ViewQuestionAttended"
            android:exported="false"
            android:parentActivityName=".ui.logIn.SecondActivity" />
        <activity
            android:name=".QuestionActivity"
            android:exported="false"
            android:parentActivityName=".CustomContest" />
        <activity
            android:name=".CustomContest"
            android:exported="false"
            android:parentActivityName=".ui.logIn.SecondActivity" />
        <activity
            android:name=".StartContest"
            android:exported="false"
            android:parentActivityName=".ui.logIn.SecondActivity" />
        <activity
            android:name=".HomePage"
            android:exported="false"
            android:label="@string/title_activity_home_page"
            android:theme="@style/Theme.Interview_Puzzle_App.NoActionBar" />
        <activity
            android:name=".ui.logIn.SecondActivity"
            android:exported="false" />
        <activity
            android:name=".ui.logIn.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>